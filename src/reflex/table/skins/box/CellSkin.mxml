<?xml version="1.0" encoding="utf-8"?>
<rx:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:rx="http://rx.reflex.io/2010">
	
	<rx:states>
		<s:State name="up" />
		<s:State name="over" />
	</rx:states>
	
	<rx:Rect width="{ width }" height="{ height }">
		<rx:fill>
			<s:SolidColor color="#AABCFE" alpha="0" alpha.over="1" />
		</rx:fill>
	</rx:Rect>
	
	<rx:Label id="label" fontFamily="Helvetica" fontSize="12" color="#666699" padding="10" />
	
</rx:Skin>
